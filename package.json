{
  "name": "famous-doc-generator",
  "version": "0.1.0",
  "description": "Documentation generator.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/Famous/famous-doc-generator.git"
  },
  "scripts": {
    "test": "npm run-script lint; ./node_modules/.bin/tap test/*.js",
    "watch": "./node_modules/.bin/nodemon --ignore node_modules/ -e js,json --exec 'npm test'",
    "lint": "./node_modules/.bin/eslint **/*.js"
  },
  "author": "Tim Chin <tim@famo.us>",
  "license": "MPLv2",
  "bugs": {
    "url": "https://github.com/Famous/widget-slider/issues"
  },
  "homepage": "https://github.com/Famous/widget-slider",
  "dependencies": {
    "async": "^0.9.0",
    "eslint": "^0.6.2",
    "highlight.js": "^8.1.0",
    "jade": "^1.4.2",
    "minimist": "^0.1.0",
    "mkdirp": "^0.5.0",
    "ncp": "^0.6.0",
    "nodemon": "^1.2.0",
    "tap": "^0.4.11",
    "tape": "^2.13.2",
    "yuidocjs": "git://github.com/timjchin/yuidoc.git#master"
  },
  "bin": {
    "famousdocs": "bin/cmd.js"
  },
  "preferGlobal": "true",
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "test": "./test"
  },
  "engines": {
    "node": ">=0.10.28",
    "npm": ">=1.4.10"
  }
}
